pipeline {
    agent any 
    stages {
        stage('Build') { 
            steps {
               echo "test 1" 
            }
        }
        stage('Test') { 
            steps {
               echo "test 2"
            }
        }
        stage('Deploy') { 
            steps {
                echo "test 3"
               script {
                 sh 'build.sh'
                 }
            }
        }
    }
}
